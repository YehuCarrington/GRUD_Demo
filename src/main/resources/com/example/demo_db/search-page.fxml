<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo_db.SearchPageController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="21.0" layoutY="51.0" text="Search Filter" />
        <Button fx:id="searchButton" layoutX="151.0" layoutY="188.0" mnemonicParsing="false" onAction="#SearchDB" text="Search" />
        <Button fx:id="backButton" layoutX="318.0" layoutY="188.0" mnemonicParsing="false" onAction="#SwitchToMain" text="Go Back" />
        <TextField fx:id="projNameTextField" layoutX="273.0" layoutY="47.0" promptText="Project Name" />
        <Text layoutX="273.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="From the drop down menu&#10;choose an attribute&#10;Then enter into the text field&#10;your query" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Text>
        <ChoiceBox fx:id="searchChoiceBox" layoutX="102.0" layoutY="47.0" prefWidth="150.0" />
    </children>

</AnchorPane>
